{
  "expo": {
    "name": "GifRandomer",
    "slug": "gif_randomer",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A single page app that allows user to see random GIFs, using React Native, GraphQL, and Apollo.",
    "githubUrl": "https://github.com/siriustom/gif_randomer"
  }
}